{
  "name": "@mikaelvesavuori/acmecorp-potted-plants-components",
  "version": "0.0.11",
  "description": "React component library with components generated and/or composed with Figmagic. Made for The newline Guide to React Component Design Systems with Figmagic.",
  "keywords": [
    "acmecorp",
    "component-library",
    "figmagic",
    "newline",
    "course-material"
  ],
  "author": "Mikael Vesavuori",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npx webpack",
    "init": "npx sb init -f && npm install --force",
    "prepublishOnly": "npm run figmagic && npm run build",
    "figmagic:init": "npx figmagic init",
    "figmagic": "npx figmagic",
    "start": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 6006"
  },
  "resolutions": {
    "react": "17"
  },
  "peerDependencies": {
    "prop-types": "latest",
    "react": "17",
    "react-dom": "17",
    "styled-components": "5"
  },
  "devDependencies": {
    "@babel/core": "7",
    "@babel/preset-env": "7",
    "@babel/preset-react": "7",
    "@storybook/addon-actions": "6.3.8",
    "@storybook/addon-essentials": "6.3.8",
    "@storybook/addon-links": "6.3.8",
    "@storybook/react": "6.3.8",
    "babel-loader": "8",
    "babel-plugin-module-resolver": "4",
    "chromatic": "5",
    "eslint": "latest",
    "eslint-config-prettier": "8",
    "eslint-plugin-prettier": "4",
    "figmagic": "4.3.3",
    "file-loader": "6",
    "prettier": "latest",
    "webpack": "4",
    "webpack-cli": "4",
    "webpack-dev-server": "3"
  }
}
